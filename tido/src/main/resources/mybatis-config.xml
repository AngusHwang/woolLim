<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.tido.project.model.vo.Project" alias="Project"/>
		<typeAlias type="com.kh.tido.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.tido.board.model.vo.Reply" alias="Reply"/>
		<typeAlias type="com.kh.tido.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.tido.visitor.model.vo.VisitorCount" alias="VisitorCount"/>		
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/project-mapper.xml"/>
		<mapper resource="mappers/board-mapper.xml"/>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/visitor-mapper.xml"/>
 	</mappers>

</configuration>